: 0;\r\n      border-radius: 50%;\r\n      box-sizing: border-box;\r\n      animation: animate 3s linear infinite;\r\n      animation-delay: 1s;\r\n    }\r\n\r\n    @keyframes animate {\r\n      0% {\r\n        width: 60%;\r\n        height: 60%;\r\n        border: 3px solid $primary_color;\r\n        opacity: 1;\r\n      }\r\n      100% {\r\n        width: 100%;\r\n        height: 100%;\r\n        opacity: 0;\r\n      }\r\n    }\r\n    &.active .line {\r\n      background: transparent;\r\n    }\r\n\r\n    &.active .line:before {\r\n      top: 0;\r\n      transform: rotate(-135deg);\r\n    }\r\n\r\n    &.active .line:after {\r\n      top: 0;\r\n      transform: rotate(135deg);\r\n    }\r\n\r\n    .line {\r\n      position: absolute;\r\n      top: 50%;\r\n      left: 50%;\r\n      transform: translate(-50%, -50%);\r\n      width: 28px;\r\n      height: 4px;\r\n      background: #fff;\r\n      border-radius: 5px;\r\n      transition: all 0.3s ease-in-out;\r\n\r\n      &:before,\r\n      &:after {\r\n        position: absolute;\r\n        content: \"\";\r\n        width: 28px;\r\n        height: 4px;\r\n        background: #fff;\r\n        border-radius: 5px;\r\n        transition: all 0.3s ease-in-out;\r\n      }\r\n\r\n      &:before {\r\n        top: -9px;\r\n      }\r\n\r\n      &:after {\r\n        top: 9px;\r\n      }\r\n    }\r\n  }\r\n\r\n  .nav__list {\r\n    position: absolute;\r\n    top: 100%;\r\n    right: -100%;\r\n    opacity: 0;\r\n    z-index: -1;\r\n    transition: all 0.3s ease-in-out;\r\n\r\n    &.open {\r\n      top: 100%;\r\n      right: -20%;\r\n      opacity: 1;\r\n    }\r\n\r\n    .list {\r\n      .list__item {\r\n        list-style: none;\r\n        text-align: center;\r\n        padding: 10px 0px;\r\n\r\n        a {\r\n          text-decoration: none;\r\n          color: #fff;\r\n          font-size: 1.1rem;\r\n          font-family: \"Roboto mono\";\r\n          position: relative;\r\n\r\n          &:before {\r\n            content: \"\";\r\n            position: absolute;\r\n            top: -7px;\r\n            left: 0%;\r\n            height: 2px;\r\n            border-radius: 3px;\r\n            width: 0%;\r\n            background: transparent;\r\n            transition: all 0.3s ease-in-out;\r\n          }\r\n          &:after {\r\n            content: \"\";\r\n            position: absolute;\r\n            bottom: -7px;\r\n            right: 0%;\r\n            height: 2px;\r\n            border-radius: 3px;\r\n            width: 0%;\r\n            background: transparent;\r\n            transition: all 0.3s ease-in-out;\r\n          }\r\n        }\r\n\r\n        &:hover a:before {\r\n          background: cyan;\r\n          width: 100%;\r\n        }\r\n        &:hover a:after {\r\n          background: cyan;\r\n          width: 100%;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n","sass\\_home.scss":".home__content {\r\n  height: 100%;\r\n  width: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n\r\n  .header {\r\n    z-index: 50;\r\n    padding: 50px;\r\n    padding-left: 30px;\r\n\r\n    .typer {\r\n      font-size: 3.5rem;\r\n      font-family: \"Poppins\";\r\n      font-weight: 600;\r\n    }\r\n    .hello {\r\n      font-size: 2rem;\r\n      font-family: \"Roboto mono\";\r\n      font-weight: bold;\r\n    }\r\n    span {\r\n      color: cyan;\r\n      font-family: \"Roboto mono\";\r\n    }\r\n\r\n    .resume {\r\n      border: none;\r\n      outline: none;\r\n      background: transparent;\r\n      border: 2px solid $primary_color;\r\n      padding: 11px 30px;\r\n      border-radius: 30px;\r\n      cursor: pointer;\r\n      font-weight: bold;\r\n      margin-top: 20px;\r\n      text-transform: uppercase;\r\n      letter-spacing: 0.2rem;\r\n      color: $primary_color;\r\n      font-size: 1rem;\r\n      transition: all 0.3s ease-in-out;\r\n      position: relative;\r\n\r\n      &:before {\r\n        content: \"\";\r\n        position: absolute;\r\n        top: 0%;\r\n        left: 0;\r\n        height: 100%;\r\n        width: 00%;\r\n        background: transparent;\r\n        transition: all 0.3s ease-in-out;\r\n        border-radius: 30px;\r\n        transform-origin: bottom;\r\n        z-index: -1;\r\n      }\r\n      &:hover:before {\r\n        background: $primary_color;\r\n        width: 100%;\r\n      }\r\n      &:hover {\r\n        color: #000;\r\n        border: nones;\r\n      }\r\n    }\r\n  }\r\n}\r\n","sass\\main.scss":"@import \"config\";\r\n@import \"navbar\";\r\n@import \"home\";\r\n\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n  background: $primary_background;\r\n  color: #ffff;\r\n  font-family: \"Roboto mono\";\r\n}\r\n\r\n::-webkit-scrollbar {\r\n  display: none;\r\n}\r\n\r\n.container {\r\n  display: flex;\r\n  flex-direction: row;\r\n  transform: rotate(90deg) translateY(-100vh);\r\n  transform-origin: top left;\r\n  width: 400vw;\r\n}\r\n.slide {\r\n  height: 100vh;\r\n  width: 100vw;\r\n}\r\n\r\n.two {\r\n  background: #52e4dc;\r\n}\r\n\r\n.three {\r\n  background: #52eb52;\r\n}\r\n\r\n.four {\r\n  background: #caf062;\r\n}\r\n\r\n.outer__container {\r\n  width: 100vh;\r\n  height: 100vw;\r\n  transform: rotate(-90deg) translateX(-100vh);\r\n  transform-origin: top left;\r\n  overflow-y: scroll;\r\n  overflow-x: hidden;\r\n  position: absolute;\r\n}\r\n\r\n@media screen and (max-width: 800px) {\r\n  .outer__container {\r\n    transform: none;\r\n    height: 100%;\r\n    width: 100%;\r\n  }\r\n  .container {\r\n    flex-direction: column;\r\n    transform: none;\r\n  }\r\n\r\n  .home__content {\r\n    .header {\r\n      padding: 30px;\r\n    }\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 500px) {\r\n  .home__content {\r\n    text-align: center;\r\n    .header {\r\n      width: 100%;\r\n      padding: 0;\r\n      .hello {\r\n        font-size: 1.3rem;\r\n        margin-bottom: 7px;\r\n      }\r\n      .typer {\r\n        font-size: 1.7rem;\r\n      }\r\n\r\n      .resume {\